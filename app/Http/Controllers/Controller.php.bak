    }


    public function generateQrCode($rack_code, $qr_text){
        $folder_path = "public/qr-code/$rack_code/";
        if(!File::isDirectory($folder_path)){
            File::makeDirectory($folder_path, 0777, true, true);
        }
        $file = $folder_path.$qr_text.".png";
        QRCode::text($qr_text)->setSize(8)->setMargin(2)->setOutfile($file)->png();
        return $file;
    }





